  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Application",
      "request": "launch",
      "mainClass": "com.ufopa.spring.Application",
      "projectName": "spring",
      "envFile": "${workspaceFolder}/dev.env",
      "vmArgs": "-Dspring.profiles.active=dev -Dspring.datasource.url=jdbc:postgresql://localhost:5432/${API_DATABASE}"
    },
    {
      "type": "java",
      "name": "Attach to Remote Program",
      "request": "attach",
      "hostName": "0.0.0.0",
      "port": 5005
    }
  ]
}
